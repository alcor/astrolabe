{
  "name": "Astrolabe",
  "short_name": "Astrolabe",
  "description": "",
  "version": "0.6",
  "icons": { "128": "img/astrolabe.png" },
  "incognito": "split",
  "chrome_url_overrides": {
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "img/tabs.png",
    "default_title": "Tab Manager",
    "default_popup": "sidebar.html"
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "Command+Shift+A"
      },
      "description": "Activate in window"
    },
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      }
    }
  },
  "permissions": [
    "tabs", "tabGroups", "storage", "bookmarks", "history" 
  ],
  "manifest_version": 3
}